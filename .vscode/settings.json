{
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "cmake.sourceDirectory": "${workspaceFolder}/Bootstrap",
    "cmake.buildDirectory": "${workspaceFolder}/Build",
    "files.associations": {
        "*.c": "cpp",
        "*.C": "c",
        "*.cpp": "cpp",
        "*.h": "cpp",
        "atomic": "cpp",
        "utility": "cpp"
    }
}